{
  "name": "csgo-price-checker",
  "productName": "CSGO Price Checker",
  "version": "2.1.3",
  "description": "CS:GO Skins Price Checker that displays the lowest price for Steam Market, Waxpeer, and Bitskins. Created by @Hitm0nLim and completed on May 30, 2020",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "publish": "electron-builder -p always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hitm0nLim/csgo-price-checker.git"
  },
  "keywords": [],
  "author": "Hitm0nLim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hitm0nLim/csgo-price-checker/issues"
  },
  "homepage": "https://github.com/Hitm0nLim/csgo-price-checker#readme",
  "devDependencies": {
    "electron": "13.5.1",
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "notp": "^2.0.3",
    "thirty-two": "^1.0.2",
    "electron-updater": "^4.6.1"
  },
  "build": {
    "appId": "com.github.Hitm0nLim.csgo-price-checker",
    "win": {
      "icon": "assets/img/icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
